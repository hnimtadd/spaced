{
  "version": 2,
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "public": true,
  "functions": {
    "api/sound/index.go": {
      "memory": 512,
      "maxDuration": 30
    }
  },
  "outputDirectory": "ui",
  "rewrites": [
    {
      "source": "/stats",
      "destination": "/stats.html"
    },
    {
      "source": "/session",
      "destination": "/session.html"
    },
    {
      "source": "/reset",
      "destination": "/reset.html"
    },
    {
      "source": "/assets/:match*",
      "destination": "/assets/:match*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
