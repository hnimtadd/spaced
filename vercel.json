{
  "version": 2,
  "public": "src",
  "routes": [
    {
      "src": "/stats",
      "dest": "/stats.html"
    },
    {
      "src": "/session",
      "dest": "/session.html"
    },
    {
      "src": "/reset",
      "dest": "/reset.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

