{
  "version": 2,
  "public": true,
  "functions": {
    "api/sound/index.go": {
      "memory": 512,
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/stats",
      "dest": "/stats.html"
    },
    {
      "src": "/session",
      "dest": "/session.html"
    },
    {
      "src": "/reset",
      "dest": "/reset.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
